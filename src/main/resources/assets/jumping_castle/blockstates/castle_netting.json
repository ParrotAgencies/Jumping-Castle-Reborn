{
  "multipart": [
    {
      "apply": {
        "model": "jumping_castle:block/castle_netting_post"
      }
    },
    {
      "when": {
        "north": "true"
      },
      "apply": {
        "model": "jumping_castle:block/castle_netting_side"
      }
    },
    {
      "when": {
        "east": "true"
      },
      "apply": {
        "model": "jumping_castle:block/castle_netting_side",
        "y": 90
      }
    },
    {
      "when": {
        "south": "true"
      },
      "apply": {
        "model": "jumping_castle:block/castle_netting_side_alt"
      }
    },
    {
      "when": {
        "west": "true"
      },
      "apply": {
        "model": "jumping_castle:block/castle_netting_side_alt",
        "y": 90
      }
    },
    {
      "when": {
        "north": "false"
      },
      "apply": {
        "model": "jumping_castle:block/castle_netting_noside"
      }
    },
    {
      "when": {
        "east": "false"
      },
      "apply": {
        "model": "jumping_castle:block/castle_netting_noside_alt"
      }
    },
    {
      "when": {
        "south": "false"
      },
      "apply": {
        "model": "jumping_castle:block/castle_netting_noside_alt",
        "y": 90
      }
    },
    {
      "when": {
        "west": "false"
      },
      "apply": {
        "model": "jumping_castle:block/castle_netting_noside",
        "y": 270
      }
    }
  ]
}